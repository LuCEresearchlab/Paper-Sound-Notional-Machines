@article{ainsworthDeFTConceptualFramework2006,
  title = {{{DeFT}}: {{A}} Conceptual Framework for Considering Learning with Multiple Representations},
  shorttitle = {{{DeFT}}},
  author = {Ainsworth, Shaaron},
  year = {2006},
  journal = {Learning and Instruction},
  volume = {16},
